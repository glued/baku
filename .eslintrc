{
  "parser": "babel-eslint",
  "plugins": [
    "flow-vars"
  ],
  "rules": {
    "strict": 0,
    "semi": [2, "never"],
    "flow-vars/define-flow-type": 1,
    "no-unused-vars":[2, {"vars": "local"}],
    "flow-vars/use-flow-type": 1,
    "no-undef":1,
    "quotes": [1, "single"],
    "no-multi-spaces": 0,
    "no-unexpected-multiline": 0,
    "curly": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-console":0
  },
  "env": {
    "es6": true,
    "browser": true
  }
}
